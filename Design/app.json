{
    "designSystem": {
      "name": "Contemporary Multi-Platform Design System",
      "version": "2.0.0",
      "description": "A versatile design system combining glassmorphism, 3D elements, and modern card layouts with emphasis on light themes and visual depth"
    },
    "colorPalette": {
      "primary": {
        "purple": "#8B5FBF",
        "blue": "#4F7EF7", 
        "darkBlue": "#2B4A8C",
        "accent": "#FF6B6B",
        "orange": "#FF8A65"
      },
      "neutral": {
        "white": "#FFFFFF",
        "lightGray": "#F8F9FA",
        "mediumGray": "#E9ECEF",
        "darkGray": "#6C757D",
        "charcoal": "#343A40",
        "black": "#000000"
      },
      "background": {
        "lightGradient": "linear-gradient(135deg, #F8F9FA 0%, #E9ECEF 100%)",
        "darkGradient": "linear-gradient(135deg, #2C2C2C 0%, #1A1A1A 100%)",
        "purpleGradient": "linear-gradient(135deg, #8B5FBF 0%, #6A4C93 100%)",
        "glassmorphism": "rgba(255, 255, 255, 0.25)",
        "cardLight": "rgba(255, 255, 255, 0.9)",
        "cardDark": "rgba(0, 0, 0, 0.8)"
      },
      "accent": {
        "pink": "#FF69B4",
        "cyan": "#00CED1",
        "mint": "#98FB98",
        "coral": "#FF7F50"
      }
    },
    "typography": {
      "fontFamily": "Inter, system-ui, -apple-system, sans-serif",
      "weights": {
        "regular": 400,
        "medium": 500,
        "semibold": 600,
        "bold": 700
      },
      "scale": {
        "hero": {
          "fontSize": "2.5rem",
          "lineHeight": "1.2",
          "fontWeight": 700,
          "letterSpacing": "-0.02em"
        },
        "title": {
          "fontSize": "1.5rem",
          "lineHeight": "1.3",
          "fontWeight": 600
  
        },
        "body": {
          "fontSize": "0.875rem",
          "lineHeight": "1.5",
          "fontWeight": 400
        },
        "caption": {
          "fontSize": "0.75rem",
          "lineHeight": "1.4",
          "fontWeight": 500
        }
      }
    },
    "spacing": {
      "scale": [4, 8, 12, 16, 20, 24, 32, 40, 48, 64, 80, 96],
      "component": {
        "cardPadding": "32px",
        "inputPadding": "16px",
        "buttonPadding": "12px 24px",
        "elementGap": "20px"
      }
    },
    "borderRadius": {
      "small": "8px",
      "medium": "12px",
      "large": "16px",
      "xlarge": "20px"
    },
    "shadows": {
      "card": "0 20px 40px rgba(0, 0, 0, 0.1)",
      "cardHover": "0 25px 50px rgba(0, 0, 0, 0.15)",
      "button": "0 4px 12px rgba(79, 126, 247, 0.3)",
      "input": "0 2px 8px rgba(0, 0, 0, 0.05)"
    },
    "effects": {
      "glassmorphism": {
        "lightTheme": {
          "background": "rgba(255, 255, 255, 0.15)",
          "backdropFilter": "blur(20px)",
          "border": "1px solid rgba(255, 255, 255, 0.2)",
          "boxShadow": "0 8px 32px rgba(31, 38, 135, 0.37)"
        },
        "darkTheme": {
          "background": "rgba(0, 0, 0, 0.25)",
          "backdropFilter": "blur(15px)",
          "border": "1px solid rgba(255, 255, 255, 0.1)"
        }
      },
      "floating3D": {
        "transform": "perspective(1000px) rotateX(5deg) rotateY(-5deg)",
        "boxShadow": "0 25px 50px rgba(0, 0, 0, 0.15)",
        "transition": "all 0.3s cubic-bezier(0.4, 0.0, 0.2, 1)"
      },
      "morphism": {
        "neomorphism": {
          "background": "#F0F0F0",
          "boxShadow": "20px 20px 60px #bebebe, -20px -20px 60px #ffffff"
        },
        "blobMorphism": {
          "borderRadius": "30% 70% 70% 30% / 30% 30% 70% 70%",
          "animation": "morph 8s ease-in-out infinite"
        }
      }
    },
    "layout": {
      "structures": {
        "heroSection": "split-content-with-visual",
        "cardGrid": "asymmetric-floating-cards", 
        "serviceSection": "illustration-with-content",
        "statsDisplay": "horizontal-metric-cards"
      },
      "composition": {
        "background": "full-viewport-with-floating-elements",
        "cardArrangement": "multi-layer-stacking",
        "visualHierarchy": "large-hero-medium-content-small-details"
      },
      "positioning": {
        "floatingCards": {
          "transform": "translate3d(x, y, z) with rotation",
          "zIndex": "layered-depth-system"
        },
        "backgroundElements": {
          "transform": "absolute positioning with parallax",
          "zIndex": "behind-content"
        }
      }
    },
    "components": {
      "navigation": {
        "style": "floating-pill-nav",
        "background": "glassmorphism-light",
        "borderRadius": "50px",
        "padding": "12px 24px",
        "backdropFilter": "blur(20px)",
        "position": "fixed-top-with-margin"
      },
      "heroSection": {
        "layout": "split-screen",
        "leftContent": {
          "title": "large-gradient-text",
          "subtitle": "medium-muted-text",
          "cta": "primary-button-group"
        },
        "rightContent": {
          "type": "3d-visual-element",
          "style": "floating-card-or-device",
          "effects": "parallax-movement"
        }
      },
      "floatingCards": {
        "baseStyle": {
          "background": "glassmorphism-adaptive",
          "borderRadius": "20px",
          "padding": "24px",
          "transform": "rotate(-2deg) translateZ(20px)",
          "boxShadow": "floating-depth"
        },
        "variants": {
          "metric": "large-number-small-label",
          "feature": "icon-title-description",
          "product": "image-title-price-cta"
        }
      },
      "statsDisplay": {
        "layout": "horizontal-grid",
        "itemStyle": {
          "background": "transparent",
          "textAlign": "center",
          "spacing": "large-gap"
        },
        "typography": {
          "number": "hero-size-bold",
          "label": "small-muted"
        }
      },
      "backgroundElements": {
        "geometric": {
          "shapes": ["circles", "blobs", "gradients"],
          "positioning": "absolute-scattered",
          "opacity": "0.1-0.3",
          "animation": "subtle-float"
        },
        "particles": {
          "type": "floating-orbs",
          "count": "3-8",
          "size": "varied-small-to-large",
          "movement": "slow-drift"
        }
      },
      "buttons": {
        "primary": {
          "background": "solid-color-or-gradient",
          "color": "white",
          "borderRadius": "12px",
          "padding": "14px 28px",
          "fontWeight": "600",
          "boxShadow": "0 4px 15px rgba(primary-color, 0.4)"
        },
        "secondary": {
          "background": "transparent",
          "border": "2px solid current-color",
          "color": "inherit",
          "borderRadius": "12px",
          "padding": "12px 26px"
        },
        "ghost": {
          "background": "glassmorphism",
          "border": "1px solid rgba(255,255,255,0.2)",
          "color": "inherit",
          "backdropFilter": "blur(10px)"
        }
      }
    },
    "patterns": {
      "visualDepth": {
        "technique": "multi-layer-z-index",
        "elements": ["background-blur", "floating-cards", "foreground-content"],
        "spacing": "z-index: 1, 10, 100 hierarchy"
      },
      "contentLayout": {
        "hero": "split-screen-with-visual",
        "features": "card-grid-asymmetric",
        "stats": "horizontal-metrics-row",
        "services": "icon-content-illustration"
      },
      "interactionStates": {
        "hover": "lift-and-glow-effect",
        "focus": "border-highlight-with-shadow",
        "active": "slight-scale-down"
      },
      "responsiveAdaptation": {
        "desktop": "side-by-side-layouts",
        "tablet": "stacked-with-reduced-spacing",
        "mobile": "single-column-full-width"
      },
      "themeVariations": {
        "light": {
          "background": "light-gradients-white-cards",
          "text": "dark-on-light",
          "effects": "subtle-shadows"
        },
        "dark": {
          "background": "dark-gradients-glass-cards", 
          "text": "light-on-dark",
          "effects": "pronounced-glows"
        }
      }
    },
    "animations": {
      "pageLoad": {
        "elements": "stagger-fade-in-up",
        "duration": "0.6s",
        "easing": "cubic-bezier(0.4, 0.0, 0.2, 1)",
        "delay": "0.1s increments"
      },
      "cardHover": {
        "transform": "translateY(-8px) scale(1.02)",
        "boxShadow": "enhanced-depth",
        "transition": "all 0.3s ease-out"
      },
      "floatingElements": {
        "keyframes": "gentle-float-rotation",
        "duration": "6s",
        "timingFunction": "ease-in-out",
        "iterationCount": "infinite"
      },
      "backgroundParticles": {
        "movement": "slow-drift-with-rotation",
        "duration": "10-20s",
        "direction": "random-paths"
      },
      "buttonInteractions": {
        "hover": {
          "transform": "translateY(-2px)",
          "boxShadow": "0 8px 25px rgba(primary, 0.4)",
          "transition": "all 0.2s ease"
        },
        "click": {
          "transform": "scale(0.98)",
          "transition": "all 0.1s ease"
        }
      },
      "morphing": {
        "blobShapes": {
          "keyframes": "border-radius-morph",
          "duration": "8s",
          "timingFunction": "ease-in-out",
          "iterationCount": "infinite"
        }
      }
    },
    "responsiveBreakpoints": {
      "mobile": "max-width: 768px",
      "tablet": "max-width: 1024px",
      "desktop": "min-width: 1025px"
    },
    "usageGuidelines": {
      "whenToUse": [
        "Modern web applications",
        "E-commerce platforms", 
        "SaaS landing pages",
        "Creative portfolios",
        "Financial services",
        "Tech product showcases"
      ],
      "designPrinciples": [
        "Layer content with purposeful depth hierarchy",
        "Use glassmorphism sparingly for premium feel",
        "Maintain strong contrast ratios for accessibility",
        "Implement smooth micro-interactions throughout",
        "Balance visual elements with functional content",
        "Prioritize light themes with dark mode alternatives",
        "Create floating, dynamic layouts that feel alive",
        "Use 3D transforms and perspective for modern appeal"
      ],
      "implementation": {
        "frameworks": "React/Vue/Angular/Svelte compatible", 
        "cssFramework": "Tailwind CSS with custom utilities",
        "dependencies": [
          "backdrop-filter support required",
          "transform3d for 3D effects", 
          "CSS custom properties for theming",
          "intersection-observer for scroll animations"
        ],
        "performance": {
          "optimize": "blur effects and transforms",
          "fallbacks": "graceful degradation for older browsers",
          "loading": "progressive enhancement approach"
        }
      },
      "accessibilityConsiderations": {
        "contrast": "4.5:1 minimum for normal text",
        "motionSensitivity": "respect prefers-reduced-motion",
        "focusManagement": "clear focus indicators",
        "semanticMarkup": "proper heading hierarchy and landmarks"
      }
    }
  }